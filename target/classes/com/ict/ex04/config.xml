<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean id="age" class="com.ict.ex04.Age"/>
	
	<bean id="mp1" class="com.ict.ex04.Myprocess">
		<constructor-arg name="name" value="고길동" />
		<constructor-arg name="jumin" value="990909" />
		<constructor-arg name="gender" value="1" />
		<constructor-arg name="age" ref="age" />
	</bean>
	<bean id="mp2" class="com.ict.ex04.Myprocess">
		<property name="name" value="김한별"></property>
		<property name="jumin" value="030505"></property>
		<property name="gender" value="4"></property>
		<property name="age" ref="age"></property>
	</bean>
</beans>
